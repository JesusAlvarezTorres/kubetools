{
	"clusterARMTemplate": {
		"githubRepo": "msazurestackworkloads/azurestack-gallery",
		"githubBranch": "ci-test",
		"folderPath": "kubernetes/template/DeploymentTemplates",
		"dvmLogFilePath": "/var/log/azure/deploy-script-dvm.log",
		"aksEngine": {
			"githubRepo": "msazurestackworkloads/aks-engine",
			"githubBranch": "azsmaster"
		},
		"deploymentMatrix": [
			{
				"agentPoolProfileCount": 3,
				"agentPoolProfilevmSize": "Standard_D5_v2",
				"masterPoolProfileCount": 1,
				"masterPoolProfilevmSize": "Standard_D5_v2",
				"storageProfile": "ManagedDisk",
				"hyperkubeImagesVersion": "1.11"
			}
		]
	},
	"diagnosis": {
		"githubRepo": "msazurestackworkloads/azurestack-gallery",
		"githubBranch": "master",
		"folderPath": "diagnosis",
		"filename": "getkuberneteslogs.sh"
	},	
	"testSuite": {
		"githubRepo": "msazurestackworkloads/kubetools",
		"githubBranch": "master",
		"applications": [{
            "name": "sqlaris",
            "deploymentVMType":"dvm",
			"deployment": {
				"scriptName" : "run_deploy.sh",
				"summaryFile" : "deploy.json"
			},
			"validation": {
				"scriptName" : "run_parse.sh",
				"summaryFile" : "validate.json"
			},
			"cleanup": {
				"scriptName" : "run_clean.sh",
				"summaryFile" : "cleanup.json"
			}
		}]
	}
}